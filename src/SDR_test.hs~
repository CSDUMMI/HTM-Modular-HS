module SDR_test
  (
    sdr_test
  ) where
import Test
import SDR
sdr_test = do 
  --Test duplicate function
  let list_with_dups = [1,2,3,1,2,3]
  let list_without_dups = [1,2,3]
  expect $
    (remove_duplicates
    list_with_dups)
    `toBe`
    list_without_dups
  -- Test s_union
    expect $ union_fail `toBe` sdr_1 where
        let sdr_1 = SDR {
              sdr_active_indicies=[8,10],
              len=16
            }
        let sdr_to_fail = SDR {
              sdr_active_indicies=[12,20],
              len=21
            }
        let sdr_2 = SDR {
              sdr_active_indicies=[9,11],
              len=16
            }
        let union_fail = s_union sdr_1 sdr_to_fail
  
  
